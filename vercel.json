{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "env": {
          "SEED_COMMAND": "node prisma/seeds/seed-admin-user.js" 
        }
      }
    ],
    "routes": [
      {
        "src": "(.*)",
        "dest": "index.js"
      }
    ],
    "crons": [{
      "path": "/cron",
      "schedule": "0 10 * * *"
    }],
    "env": {
      "SEED_COMMAND": "node prisma/seeds/seed-admin-user.js" 
    }
  }
  